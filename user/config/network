
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option ifname 'eth0'
	option force_link '1'
	option type 'bridge'
	option proto 'STATIC'
	option ipaddr '192.168.0.7'
	option netmask '255.255.255.0'
	option gateway '192.168.0.1'
	option ip6assign '60'
	option dns '208.67.222.222'
	option dns2 '8.8.8.8'
	option macaddr 'D8:B0:4C:00:00:01'

config interface 'wan_wired'
	option ifname 'eth1'
	option proto 'dhcp'
	option hostname ''
	option nettype 'bridge'
	
config interface 'pptp'
	option enabled 'OFF'
	option serveraddr ' '
	option username 'testvpn'
	option password 'testvpn'
	option dataencrypt 'require-mppe-128'
	option auth 'noauth'
	option lcpecho '300'
	option mtu '1500'

config interface 'gre'
	option enabled 'OFF'
	option tunnelname 'gre1'
	option iftype 'static' 

